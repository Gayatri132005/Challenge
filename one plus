#include<iostream> 
using namespace std;

 void plusOne( int arr[],int n){
     for( int i=0;i<n;i++){
         if(arr[i]==arr[n-1]){
             cout<<arr[i]+1<<" ";
         }else{
             cout<<arr[i]<<" ";
         }
        
     }
 }
  int main(){
       int arr[]={1,2,3,5};
       int n=sizeof(arr)/sizeof(int);
       
       plusOne(arr,n);
      
  }
